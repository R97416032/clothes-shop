.num-box 
{
	width: 168rpx;
	height: 46rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	position: absolute;
	right: 40rpx;
}
.num-box-image
{
	width: 46rpx;
	height: 44rpx;
	text-align: center;
	line-height: 44rpx;
	border: 1rpx solid #CFCFCF;
	font-size: 38rpx;
}
.num-box-num
{
	width: 76rpx;
	height: 44rpx;
